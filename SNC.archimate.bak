        <child xsi:type="archimate:DiagramObject" id="6705ae89-f883-4808-b427-7f84636e76fe" targetConnections="4c3bcf5d-fd54-4ef4-9cbc-4475592b51b5" archimateElement="d22a24ab-bf8f-44d8-862d-d9c29482f4b9">
          <bounds x="564" y="128" width="180" height="55"/>
